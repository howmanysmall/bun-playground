{
	"bugs": {
		"url": "https://github.com/howmanysmall/bun-playground/issues"
	},
	"dependencies": {
		"@inquirer/prompts": "^7.5.3",
		"chalk": "^5.4.1",
		"cli-table3": "^0.6.5",
		"confbox": "^0.2.2",
		"css-select": "^5.1.0",
		"domhandler": "^5.0.3",
		"htmlparser2": "^10.`0.0",
		"termost": "^1.4.0",
		"zod": "^3.25.56",
		"zod-validation-error": "^3.4.1"
	},
	"description": "A Bun CLI template.",
	"devDependencies": {
		"@biomejs/biome": "^2.0.5",
		"@mitata/counters": "^0.0.8",
		"@types/bun": "latest",
		"concurrently": "^9.1.2",
		"mitata": "^1.0.34",
		"rimraf": "^6.0.1"
	},
	"homepage": "https://github.com/howmanysmall/bun-playground#readme",
	"license": "MIT",
	"module": "src/index.ts",
	"name": "bun-playground",
	"peerDependencies": {
		"typescript": "^5"
	},
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/howmanysmall/bun-playground.git"
	},
	"scripts": {
		"build": "bun build ./src/index.ts --target bun --compile --outfile out/bun-playground",
		"build-mini": "bun build ./src/index.ts --target bun --minify --compile --outfile out/bun-playground",
		"build-prod": "bun build --compile --minify --sourcemap ./src/index.ts --outfile out/bun-playground",
		"clean-install": "bun x rimraf node_modules bun.lock && bun install -f",
		"create-prompt": "code2prompt --exclude \"*.test.ts\" src",
		"dev": "bun run --watch src/index.ts",
		"dev-hot": "bun run --hot --watch src/index.ts",
		"lint": "bun x biome ci",
		"start": "bun run src/index.ts",
		"start:prod": "NODE_ENV=production bun run src/index.ts",
		"test": "bun test"
	},
	"type": "module",
	"version": "0.0.1-alpha.0"
}
